#include <iostream>
#include <cstdio>

class point{
public:
	void setx(double v){
		x=v;
	}
	double getx(){
		return x;
	}
private:
	double x,y;
};

point operator+(point&p1,point&p2){
	point sum = {
			p1.x+p2.x, p1.y+p2.y
	};

	return sum;
}

ostream& operator<<(ostream& out, const point& p){
	out<<"("<<p.x<<","<<p.y<<")";
	return out;
}
int main(){

}

